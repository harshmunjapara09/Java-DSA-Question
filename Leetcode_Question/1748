class Solution {
    public int sumOfUnique(int[] nums) {
        int sumOfUnique[] =  new int[101];
        for(int i=0;i<nums.length;i++){
            sumOfUnique[nums[i]]++;
        }
        int sum=0;
        for(int i=1;i<101;i++){
            if(sumOfUnique[i]==1){
                sum+=i;
            }
        }

        return sum;
    }
}