class Solution {
    public boolean arrayStringsAreEqual(String[] word1, String[] word2) {
        String s1 = "";
        String s2 = "";

        s1 = convertSting(word1);
        s2 = convertSting(word2);

        return s1.equals(s2);
    }
    public static String convertSting(String [] arr){
        String ans = "";
        for (int i=0;i< arr.length;i++){
            ans+=arr[i];
        }
        return ans;
    }
}