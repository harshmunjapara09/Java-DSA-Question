class Solution {
    public int xorOperation(int n, int start) {
        int [] ans  = new int[n];
        int ans1= 0;
        for (int i = 0; i < n; i++) {
            ans[i] = start + 2 * i;
        }
        for (int i = 0; i <ans.length; i++) {
          ans1 ^= ans[i];
        }
        return ans1;
    }
}